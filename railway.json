{
    "$schema": "https://railway.app/railway.schema.json",
    "build": {
        "builder": "NIXPACKS"
    },
    "deploy": {
        "builder": "node",
        "startCommand": "node yumeko.js",
        "numReplicas": 1,
        "restartPolicyType": "ALWAYS"
    },
    "env": {
        "BOT_TOKEN": {
            "secret": true,
            "required": true
        },
        "API_KEY": {
            "secret": true,
            "required": true
        }
    }
}
